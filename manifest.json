{
	"manifest_version" : 2,
	"name" : "emojicon",
	"description" : "list of different emojis",
	"version" : "1.0.0",
	"icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
	"browser_action" : {
		"default_popup": "popup.html",
		"default_icon" : "icon128.png"
	},
	"permissions": ["activeTab"]
	
}